{{#if table-basic}}
    {{#each table-basic}}

        <div class="media">
            <div class="media-left">
                <a href="#">
                    <img alt="64x64" class="media-object" data-src="holder.js/64x64" style="width: 64px; height: 64px;" src="data:image/svg+xml;base64,"
                        data-holder-rendered="true">
                </a>
            </div>
            <div class="media-body">
                <h4 class="media-heading">{{First}} {{Last}}</h4>
                <p>
                    Lorem ipsum dolor sit amet, consectetur ...
                </p>
                <div class="progress is-xs ma0 mt3">
                    <div class="progress-bar" role="progressbar" aria-valuenow="{{Score}}" aria-valuemin="0" aria-valuemax="100" style="width: {{Score}}%;">
                        <span class="sr-only">{{Score}}% Complete</span>
                    </div>
                </div>
            </div>
        </div>
    {{/each}}
{{else}}
    <div class="alert alert-danger" role="alert">NO DATA</div>
{{/if}}
